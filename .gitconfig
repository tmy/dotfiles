[user]
	name = Akinori Tomita
	email = REPLACE@example.com
[gui]
	recentrepo = /Users/tommy/Projects/redmine
	fontui = -family \"ヒラギノ角ゴ ProN W3\" -size 13 -weight normal -slant roman -underline 0 -overstrike 0
	fontdiff = -family セプテンバーＭ-等幅 -size 14 -weight normal -slant roman -underline 0 -overstrike 0
[color]
	diff = auto
	status = auto
	branch = auto
[merge]
    keepBackup = false
    tool = p4merge
[mergetool "p4merge"]
    cmd = p4merge "$BASE" "$LOCAL" "$REMOTE" "$MERGED"
    keepTemporaries = false
    trustExitCode = false
    keepBackup = false
;[diff]
;    external = p4diff
[alias]
    st = status
    ci = commit
    co = checkout
    br = branch
    di = diff
