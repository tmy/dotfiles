# -*- sh -*-

set tcsh_major=`echo $tcsh | cut -f 1 -d.`
set tcsh_minor=`echo $tcsh | cut -f 2 -d.`

foreach subrc (local "${OSTYPE}" environment applications completions)
    if (-r "${HOME}/.tcshrc-$subrc") then
        source "${HOME}/.tcshrc-$subrc"
    endif
end

unset subrc
unset tcsh_major
unset tcsh_minor
